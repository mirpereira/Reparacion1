<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.myaplicacion.hp.repair_pc" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Welcome"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Opciones"
            android:label="@string/title_activity_opciones" >
        </activity>
        <activity
            android:name=".Ayuda"
            android:label="@string/title_activity_ayuda"
            android:parentActivityName=".Opciones" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.myaplicacion.hp.repair_pc.Opciones" />
        </activity>
        <activity
            android:name=".Consulta"
            android:label="@string/title_activity_consulta"
            android:parentActivityName=".Opciones" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.myaplicacion.hp.repair_pc.Opciones" />
        </activity>
        <activity
            android:name=".Proceso"
            android:label="@string/title_activity_proceso"
            android:parentActivityName=".Consulta" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.myaplicacion.hp.repair_pc.Consulta" />
        </activity>
    </application>

</manifest>
